{
    "compilerOptions": {
      "target": "ES2019",
      "module": "CommonJS",
      "declaration": true,
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "baseUrl": "src",
    },
    "include": ["src/**/*"]
  }
  